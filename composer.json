{
  "name": "tmconsulting/payum-uniteller-gateway",
  "type": "library",
  "description": "The Payum Uniteller extension.",
  "keywords": [
    "payment",
    "uniteller",
    "processing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roquie",
      "email": "roquie0@gmail.com"
    },
    {
      "name": "Payum project",
      "homepage": "http://payum.org/"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "payum/core": "^1.4",
    "tmconsulting/uniteller-php-sdk": "0.1.*"
  },
  "require-dev": {
    "payum/core": "^1.4@dev",
    "php-http/guzzle6-adapter": "^1.0",
    "symfony/var-dumper": "^3.2",
    "phpunit/phpunit": "~5.7"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Payum\\Uniteller\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Payum\\Uniteller\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/phpunit/phpunit/phpunit",
    "coverage": "./vendor/phpunit/phpunit/phpunit --coverage-text"
  }
}
